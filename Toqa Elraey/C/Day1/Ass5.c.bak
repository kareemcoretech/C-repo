#include <stdio.h>
#include <math.h>

int main(){
	float x, y, check1, check2;
	
	printf("Please enter first number: ");
	scanf("%f", &x);
	
	printf("Please enter second number: ");
	scanf("%f", &y);
	
	check1 = x / y;
	check2 = round(check1);
	
	if(check1 == check2){
		printf("The first number is a multiple of the second.");
	}
	else{
		printf("The first number isn't a multiple of the second.");
	}
}